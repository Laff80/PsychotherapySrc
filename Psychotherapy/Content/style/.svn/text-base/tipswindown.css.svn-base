#windownbg {
	display: none;
	position: absolute;
	width: 100%;
	height: 100%;
	background: #000;/*body-bg*/
	top: 0;
	left: 0;
}
#windown-box {
	position: fixed;
	_position: absolute;/*For IE 6.0*/
	padding:6px;
	background: #F4F4F4;
	text-align: left;
}
#windown-box-border
{
    border:1px solid #BBBBBB;
}
#windown-title {
	position: relative;
	height: 32px;
	overflow: hidden;
    border-bottom:1px solid #E5E5E5;
}
#windown-title h2 {
	position: relative;
	left: 10px;
	top: 8px;
	font-size:14px;
	color:#3F3F3F;
	font-weight:bolder;
}
#windown-close {
	position: absolute;
	right: 10px;
	top: 8px;
	width: 17px;
	height: 17px;
	text-indent: -10em;
	overflow: hidden;
	background: url(../images/pop_close.png) no-repeat;
	cursor: pointer;
}
#windown-content-border {
	position: relative;
	top: 0px;
	padding: 5px 0 5px 5px;
	background: #FFF;
}
#windown-content {
	position: relative;
	overflow: auto;
	
}
#windown-content img,#windown-content iframe{
	display: block;
}
#windown-content .loading {
	position: absolute;
	left: 50%;
	top: 50%;
	margin-left: -8px;
	margin-top: -8px;
}