<%@ Page Title="" Language="C#" MasterPageFile="~/Views/Shared/ManagerSite.Master"
    Inherits="System.Web.Mvc.ViewPage<Insourcia.Psychotherapy.Model.Department>" %>

<asp:Content ID="Content1" ContentPlaceHolderID="TitleContent" runat="server">
    Department Infomation
</asp:Content>
<asp:Content ID="Content2" ContentPlaceHolderID="MainContent" runat="server">
    <table class="table-form-style">
        <tr>
            <th>
                DepartmentID
            </th>
            <td>
                <%: Model.DepartmentID %>
            </td>
        </tr>
        <tr>
            <th>
                DepartmentName
            </th>
            <td>
                <%: Model.DepartmentName %>
            </td>
        </tr>
        <tr>
            <th>
                Description
            </th>
            <td>
                <%: Model.Description %>
            </td>
        </tr>
        <tr>
            <th>
                ManagerUserID
            </th>
            <td>
                <%: Model.ManagerUserID %>
            </td>
        </tr>
        <tr>
            <th>
                TimeCreated
            </th>
            <td>
                <%: String.Format("{0:g}", Model.TimeCreated) %>
            </td>
        </tr>
        <tr>
            <th>
                LastModified
            </th>
            <td>
                <%: String.Format("{0:g}", Model.LastModified) %>
            </td>
        </tr>
        <tr>
            <th>
                LastModifiedBy
            </th>
            <td>
                <%: Model.LastModifiedBy %>
            </td>
        </tr>
        <tr>
            <th>
                IsValid
            </th>
            <td>
                <%: Model.IsValid %>
            </td>
        </tr>
    </table>
</asp:Content>
<asp:Content ID="Content3" ContentPlaceHolderID="HeadContent" runat="server">
</asp:Content>
<asp:Content ID="Content4" ContentPlaceHolderID="phToolBar" runat="server">
</asp:Content>
<asp:Content ID="Content5" ContentPlaceHolderID="phNavigation" runat="server">
</asp:Content>
